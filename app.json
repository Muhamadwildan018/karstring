{
  "name": "Dan String Session Generator",
  "description": "Telegram bot to generate pyrogram string session",
  "logo": "https://telegra.ph//file/01a2da81ad82d6d8b90b7.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "15"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/Muhamadwildan018/karstring",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": "6621888094:AAFiXgDSPC2PwFnlU_AqFptimOser6AuJmE"
    },
    "MUST_JOIN": {
      "description": "Add username/id of a channel/group",
      "required": true,
      "value": "Disney_storeDan"
      
    },
    "OWNER_ID": {
      "description": "Telegram id of the person who will known as owner of the bot",
      "required": true,
      "value": ""
    }
  }
}
